        <script type='text/javascript'>
        var i = 1;
        
        function getData(){
                if(i < 100){
                    $.get( "/item/"+i, function(data) {
                    console.log(data);
                    i++;
                    markers = JSON.stringify(data);
                    $('#div1').append("<div>"+markers+"</div>");
                    console.log(i);
                    if (i > 20){
                        $("#div1".find('div:first').remove();});
                    }
                });
        }
             
       </script>
    </head>
    
    <body>
       <div id="div1"> </div>
        <script>
            getData();
            var timeout= setInterval (getData, 500);    
        </script>
        </body>
</html>